<resources>

    <!-- Base application theme. -->
    <style name="AppTheme" parent="Theme.MaterialComponents">

    </style>

    <!--Theme 2: Dark Theme + some more color attributes-->
    <style name="AppTheme.Dark" parent="Theme.MaterialComponents">
        <item name="colorPrimary">#66BB6A</item>
        <item name="colorPrimaryDark">#549C57</item>
        <item name="colorAccent">#0086D4</item>
        <item name="colorSecondary">#00796B</item>
        <item name="colorError">#B2002A</item>
    </style>

    <!-- Cyan application theme. -->
    <style name="AppTheme.PureBlack" parent="AppTheme">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">#000000</item>
        <item name="colorPrimaryDark">#000000</item>
        <item name="colorAccent">#FFFF</item>
        <item name="android:textColor">@color/colorPrimaryText</item>
        <item name="android:colorBackground">@color/colorBackgroundPure</item>
        <item name="colorSecondary">#00796B</item>
        <item name="colorError">#B2002A</item>
    </style>

